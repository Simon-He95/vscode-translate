{
  "publisher": "simonhe",
  "name": "vscode-translate-simon",
  "displayName": "VSCode Translate Simon - 翻译插件",
  "version": "0.0.21",
  "packageManager": "pnpm@8.15.9",
  "description": "VSCode Translate - 实时翻译插件，支持悬停翻译和选中翻译",
  "author": "Simon He <https://github.com/Simon-He95>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/vscode-translate#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simon-He95/vscode-translate"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/vscode-translate/issues"
  },
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "keywords": [
    "vscode-translate",
    "translate",
    "翻译",
    "实时翻译",
    "悬停翻译",
    "选中翻译",
    "VSCode 插件"
  ],
  "categories": [
    "Other",
    "Translation",
    "Productivity"
  ],
  "main": "./dist/index.js",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.77.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-translate.copyText",
        "title": "复制文案"
      }
    ]
  },
  "scripts": {
    "dev": "pnpm build --watch",
    "test": "vitest",
    "build": "tsup src/index.ts --external vscode",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@simon_he/translate": "^0.0.6",
    "@types/node": "^18.19.67",
    "@types/vscode": "^1.95.0",
    "@vscode-use/utils": "0.1.46",
    "bumpp": "^9.9.0",
    "eslint": "^8.57.1",
    "pnpm": "^8.15.9",
    "tsup": "^6.7.0",
    "typescript": "^5.7.2",
    "vitest": "^0.29.8",
    "@vscode/vsce": "^3.2.2"
  }
}
